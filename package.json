{
  "name": "gtfsimport",
  "description": "CLI tool to import GTFS feeds from .zip to PostgreSQL database.",
  "version": "0.0.1",
  "author": "Daniel Heppner @danielhep",
  "bin": {
    "gtfsimport": "./bin/run"
  },
  "bugs": "https://github.com/danielhep/gtfsimport/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^2",
    "cli-ux": "^5.4.5",
    "dotenv": "^8.2.0",
    "pg": "^7.18.2",
    "slonik": "^22.4.4",
    "slonik-sql-tag-raw": "^1.0.2"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "globby": "^10",
    "standard": "^14.3.3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "/src"
  ],
  "homepage": "https://github.com/danielhep/gtfsimport",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "src/index.js",
  "oclif": {
    "commands": "./src/commands",
    "bin": "gtfsimport",
    "plugins": [
      "@oclif/plugin-help"
    ],
    "hooks": {
      "init": "./src/hooks/init/loadEnv"
    }
  },
  "repository": "danielhep/gtfsimport",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint .",
    "prepack": "oclif-dev manifest && oclif-dev readme",
    "test": "echo NO TESTS",
    "version": "oclif-dev readme && git add README.md"
  }
}
